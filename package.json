{
  "name": "lepton",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "repository": "https://github.com/toodols/lepton.git",
  "scripts": {
    "compile": "rm -rf out && tsc --project ./lepton-client/tsconfig.json && tsc --project ./tsconfig.server.json",
    "dev": "npm run compile && node --inspect --experimental-specifier-resolution=node ./out/index.js",
    "dropcollections": "tsc --project ./tsconfig.server.json && node --experimental-specifier-resolution=node ./out/dropall.js",
    "test": "tsc --project ./tsconfig.server.json && node --experimental-specifier-resolution=node ./out/test.js",
    "build-markup": "wasm-pack build markup --target web",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "installstuff": "cd lepton-client && npm install && cd .. && npm install"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@reduxjs/toolkit": "^1.8.2",
    "@socket.io/admin-ui": "^0.3.0",
    "@socket.io/redis-adapter": "^7.2.0",
    "body-parser": "^1.20.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "i18next": "^21.8.5",
    "i18next-browser-languagedetector": "^6.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.6.0",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.16.9",
    "react-infinite-scroll-component": "^6.1.0",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.2",
    "react-select": "^5.3.2",
    "redis": "^4.1.0",
    "sass": "^1.52.1",
    "socket.io": "^4.5.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "17.0.36",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/react-modal": "^3.13.1",
    "@types/yargs": "^17.0.10",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
